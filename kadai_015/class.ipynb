        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "      self.name = name\n",
        "      self.age = age\n",
        "\n",
        "    # メソッドを定義する\n",
        "    def printinfo(self):\n",
        "        print(f\"name:{self.name}, age:{self.age}\")\n",
        "\n",
        "hogehoge = Human(\"侍花子\", 20)\n",
        "\n",
        "# 属性にアクセスし、値を出力する\n",
        "hogehoge.printinfo()"
      ]
    }
